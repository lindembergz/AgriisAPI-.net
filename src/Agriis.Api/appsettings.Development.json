{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=agriis_dev_db;Username=postgres;Password=postgres;Port=5432;SSL Mode=Disable"
  },
  "JwtSettings": {
    "SecretKey": "development-secret-key-that-should-be-at-least-32-characters-long-for-testing"
  },
  "AwsSettings": {
    "AccessKey": "development-access-key",
    "SecretKey": "development-secret-key",
    "Region": "us-east-1",
    "S3BucketName": "agriis-dev-files"
  },
  "SerproSettings": {
    "BaseUrl": "https://gateway.apiserpro.serpro.gov.br",
    "ConsumerKey": "dev-consumer-key",
    "ConsumerSecret": "dev-consumer-secret"
  },
  "HangfireSettings": {
    "ConnectionString": "Host=localhost;Database=agriis_dev_hangfire;Username=postgres;Password=postgres;Port=5432"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "CorsSettings": {
    "AllowedOrigins": [ "http://localhost:3000", "https://localhost:3000", "http://localhost:8080" ],
    "AllowedMethods": [ "*" ],
    "AllowedHeaders": [ "*" ],
    "AllowCredentials": true
  }
}
