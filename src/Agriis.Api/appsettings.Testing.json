{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=agriis_test_db;Username=postgres;Password=postgres;Port=5432;SSL Mode=Disable"
  },
  "JwtSettings": {
    "SecretKey": "testing-secret-key-that-should-be-at-least-32-characters-long-for-unit-tests",
    "TokenExpirationInDays": 1,
    "RefreshTokenExpirationInDays": 1
  },
  "AwsSettings": {
    "AccessKey": "test-access-key",
    "SecretKey": "test-secret-key",
    "Region": "us-east-1",
    "S3BucketName": "agriis-test-files"
  },
  "SerproSettings": {
    "BaseUrl": "https://mock-serpro-api.test",
    "ConsumerKey": "test-consumer-key",
    "ConsumerSecret": "test-consumer-secret"
  },
  "HangfireSettings": {
    "ConnectionString": "Host=localhost;Database=agriis_test_hangfire;Username=postgres;Password=postgres;Port=5432"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "CorsSettings": {
    "AllowedOrigins": [ "*" ],
    "AllowedMethods": [ "*" ],
    "AllowedHeaders": [ "*" ],
    "AllowCredentials": true
  },
  "HealthChecks": {
    "Enabled": false
  }
}