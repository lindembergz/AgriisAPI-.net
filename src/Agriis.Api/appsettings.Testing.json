{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=DBAgriis_Test;Username=postgres;Password=RootPassword123;Port=5432;SSL Mode=Disable"
  },
  "JwtSettings": {
    "SecretKey": "testing-secret-key-that-should-be-at-least-32-characters-long-for-testing"
  },
  "AwsSettings": {
    "AccessKey": "test-access-key",
    "SecretKey": "test-secret-key",
    "Region": "us-east-1",
    "S3BucketName": "agriis-test-files"
  },
  "SerproSettings": {
    "BaseUrl": "https://gateway.apiserpro.serpro.gov.br",
    "ConsumerKey": "test-consumer-key",
    "ConsumerSecret": "test-consumer-secret"
  },
  "HangfireSettings": {
    "ConnectionString": "Host=localhost;Database=DBAgriis_Test;Username=postgres;Password=RootPassword123;Port=5432"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.EntityFrameworkCore": "Error",
        "System": "Error",
        "Hangfire": "Error",
        "Agriis": "Information"
      }
    },
    "Properties": {
      "Environment": "Testing"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/test/agriis-test-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 3,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.EntityFrameworkCore": "Error",
      "Agriis": "Information"
    }
  },
  "CorsSettings": {
    "AllowedOrigins": [ "*" ],
    "AllowedMethods": [ "*" ],
    "AllowedHeaders": [ "*" ],
    "AllowCredentials": false
  },
  "HealthChecks": {
    "Enabled": false
  },
  "RequestLogging": {
    "Enabled": false,
    "LogRequestBody": false,
    "LogResponseBody": false
  }
}